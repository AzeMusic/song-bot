{
    "name": "Songaz",
    "description": "Telegram song Bot Azərbaycan",
    "env": {
        "API_ID": {
            "description": "Bunu burdan alın my.telegram.org.",
            "required": true
        },
        "API_HASH": {
            "description": "Bunu burdan alın from my.telegram.org.",
            "required": true
        },
        "BOT_TOKEN": {
            "description": "@Botfather dən alınan TOKEN bura yapşdır",
            "required": true
        },
        "OWNER_ID": {
            "description": "Bot sahibinin ID si",
            "required": true
        },
        "START_MSG": {
            "description": "Salam! [{}](tg://user?id={})",
            "required": true
        },
        "BOT_ADI": {
            "description": "Botun istifadeci adi",
            "required": true
        }
    },
    "buildpacks": [
        {
            "url": "heroku/python"
        }
    ],
    "addons": [
        {
            "plan": "heroku-postgresql",
            "options": {
                "version": "12"
            }
        }
    ],
    "formation": {
        "worker": {
            "quantity": 1,
            "size": "free"
        }
    }
}
