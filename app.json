{
    "name": "Songaz",
    "description": "Telegram song Bot Azərbaycan",
    "env": {
        "API_ID": {
            "description": "Bunu burdan alın my.telegram.org.",
            "required": true,
            "value": "1592178"
        },
        "API_HASH": {
            "description": "Bunu burdan alın from my.telegram.org.",
            "required": true,
            "value": "dd23441effcbb9037a5f8afc863e64ea"
        },
        "BOT_TOKEN": {
            "description": "@Botfather dən alınan TOKEN bura yapşdır",
            "required": true
        },
        "START_MSG": {
            "description": "Start Mesaji",
            "required": true,
            "value": "Salam! [{}](tg://user?id={})"
        },
        "BOT_ADI": {
            "description": "Botun istifadeci adi",
            "required": true,
            "value": "songazbot"
        },
        "HELP": {
            "description": "/help Mesaji düzəldə bilərsiniz",
            "required": false,
            "value": "Burda Her kes ucun olan help mesaji olacaq"
        }
    },
    "buildpacks": [
        {
            "url": "heroku/python"
        }
    ],
    "addons": [
        {
            "plan": "heroku-postgresql",
            "options": {
                "version": "12"
            }
        }
    ],
    "formation": {
        "worker": {
            "quantity": 1,
            "size": "free"
        }
    }
}
